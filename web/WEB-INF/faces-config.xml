<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
  <navigation-rule>
      <from-view-id>/listRegions.xhtml</from-view-id>
      <navigation-case>
          <from-action>#{regionsBean.newRegion}</from-action>
          <from-outcome>success</from-outcome>
          <to-view-id>/newRegion.xhtml</to-view-id>
      </navigation-case>
      <navigation-case>
          <from-action>#{regionsBean.editRegion(region)}</from-action>
          <from-outcome>success</from-outcome>
          <to-view-id>/editRegion.xhtml</to-view-id>
      </navigation-case>

  </navigation-rule>
    <navigation-rule>
        <from-view-id>/newRegion.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/listRegions.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/editRegion.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/listRegions.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>